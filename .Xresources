! -- urxvt config by SlangItUp --!

! -- Fonts -- !

URxvt.font:xft:JetBrains Mono:style=Regular:pixelsize=18
URxvt*boldFont:xft:JetBrains Mono:style=Bold:pixelsize=18
URxvt*italicFont:xft:JetBrains Mono:style=Italic:pixelsize=18
URxvt*boldItalicFont:xft:JetBrains Mono:style=Bold Italic:pixelsize=18 
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

! -- Scroll Bar -- !
URxvt.scrollBar:	false

! -- Window -- !

URxvt.title: Console

! -- Cursor -- !
URxvt.cursorColor: #ffe
URxvt.cursorBlink: true

! -- per extensions -- !

URxvt*perl-ext:
URxvt*perl-ext-common:

! -- Colors -- !

URxvt*colorUL:  blue

*.foreground:   #ababab
*.background:   #000000
*.cursorColor:  #ababab

! black
*.color0:       #000000
*.color8:       #343434

! red
*.color1:       #e92f2f
*.color9:       #e92f2f

! green
*.color2:       #0ed839
*.color10:      #0ed839

! yellow
*.color3:       #dddd13
*.color11:      #dddd13

! blue
*.color4:       #3b48e3
*.color12:      #3b48e3

! magenta
*.color5:       #f996e2
*.color13:      #f996e2

! cyan
*.color6:       #23edda
*.color14:      #23edda

! white
*.color7:       #ababab
*.color15:      #f9f9f9

